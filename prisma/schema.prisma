generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Product {
  id             Int      @id @default(autoincrement())
  category       String
  service        String
  size           String
  unitPrice      Float
  bulkPrice      Float
  turnaroundTime String
  notes          String?
  createdAt      DateTime @default(now())
  updatedAt      DateTime @updatedAt
}
